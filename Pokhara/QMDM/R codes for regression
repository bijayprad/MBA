###########################
y  = c(9, 6, 4, 5, 9, 6, 6, 9, 5, 7)
x1 = c(19, 18, 19, 18, 20, 20, 15, 16, 18, 13)
x2 = c(20, 5, 19, 6, 5, 12, 8, 15, 19, 8)
lm(y~x1+x2)
summary(lm(y~x1+x2))
###############################
df <- data.frame(
  y  = c(9, 6, 4, 5, 9, 6, 6, 9, 5, 7),
  x1 = c(19, 18, 19, 18, 20, 20, 15, 16, 18, 13),
  x2 = c(20, 5, 19, 6, 5, 12, 8, 15, 19, 8),
  D1 = c(0, 1, 0, 0, 0, 0, 1, 0, 1, 0),
  D2 = c(0, 0, 1, 1, 0, 0, 0, 1, 0, 1))
model1=lm(y~x1+x2+D1+D2
summary(model1)
